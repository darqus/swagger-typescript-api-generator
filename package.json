    "generate-and-copy": "./generate-and-copy.sh"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "api",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
